{
  "name": "@tralsejr/routex",
  "version": "0.0.0",
  "description": "Express router automator for dinosaurs.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prestart": "npm test",
    "build": "tsup",
    "pub": "tsup && npm publish --access=public",
    "release": "standard-version && git push --follow-tags origin main",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major && git push --follow-tags origin main"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tralsejr/routex.git"
  },
  "author": "Gio Kent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tralsejr/routex/issues"
  },
  "homepage": "https://github.com/tralsejr/routex#readme",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "standard-version": "^9.5.0",
    "supertest": "^7.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@tralse/developer-logs": "^1.2.0"
  }
}
